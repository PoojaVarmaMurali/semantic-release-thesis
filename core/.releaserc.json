{
  "tagFormat": "core-v${version}",
  "branches": [
    "main",
    { 
      "name": "develop",
      "channel": "dev",
      "prerelease": true
  }
      
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "sed -i 's/__version__ = \\\".*\\\"/__version__ = \\\"${nextRelease.version}\\\"/' __version__.py",
        "publishCmd": "python ../core/generate_release_notes.py"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["CHANGELOG.md", "__version__.py"],
        "message": "chore(release): core-v${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ]
}
